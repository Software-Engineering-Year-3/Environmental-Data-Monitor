{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "files": ["**/*.csproj"],
          "exclude":["bin/**","obj/**"]
        }
      ]
   }
  ],
  "build": {
    "content": [
      {
        "files": ["**/*.md", "api/**.yml", "toc.yml"]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "DocumentationHTML",
      "_appTitle": "DocumentationHTML",
      "_enableSearch": true,
      "pdf": true
    }
  }
}